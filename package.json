{
  "name": "project-files",
  "main": "./lib/main",
  "version": "0.1.6",
  "description": "The list of projects and files",
  "dependencies": {
    "atom-select-list": "^0.8.1",
    "cson": "^7.20.0",
    "fuzzaldrin": "^2.1.0",
    "fuzzaldrin-plus": "^0.6.0",
    "glob": "^7.2.0",
    "minimatch": "^5.0.1"
  },
  "keywords": [
    "bacadra",
    "files",
    "manager",
    "project",
    "path",
    "fuzzy",
    "finder",
    "autocomplete"
  ],
  "homepage": "https://github.com/bacadra/atom-project-files",
  "url": "https://github.com/bacadra/atom-project-files",
  "repository": "https://github.com/bacadra/atom-project-files",
  "consumedServices": {
    "file-icons.element-icons": {
      "versions": {
        "1.0.0": "consumeElementIcons"
      }
    },
    "atom.file-icons": {
      "versions": {
        "1.0.0": "consumeClassIcons"
      }
    },
    "tree-view": {
      "versions": {
        "^1.0.0": "consumeTreeView"
      }
    }
  },
  "providedServices": {
    "autocomplete.provider": {
      "versions": {
        "4.0.0": "getProvider"
      }
    }
  },
  "activationCommands": {
    "atom-workspace": [
      "project-files:paths-toggle",
      "project-files:paths-cache",
      "project-files:projects-toggle",
      "project-files:projects-cache",
      "project-files:projects-edit",
      "project-files:recent-toggle"
    ],
    "atom-text-editor": [
      "project-files:open-externally",
      "project-files:show-in-folder"
    ],
    "atom-text-editor[data-grammar~=latex]": [
      "project-files:open-TeX-PDF-internally",
      "project-files:open-TeX-PDF-externally"
    ],
    ".tree-view": [
      "project-files:open-externally",
      "project-files:show-in-folder"
    ]
  },
  "license": "GPL-3.0-or-later"
}
