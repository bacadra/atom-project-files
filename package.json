{
  "name": "project-files",
  "main": "./lib/main",
  "version": "0.0.6",
  "description": "The list of projects and files",
  "engines": {
    "atom": ">=1.56.0 <2.0.0"
  },
  "keywords": [
    "bacadra",
    "files",
    "manager",
    "project",
    "path",
    "fuzzy",
    "finder"
  ],
  "homepage": "https://github.com/bacadra/atom-project-files",
  "url": "https://github.com/bacadra/atom-project-files",
  "repository": "https://github.com/bacadra/atom-project-files",
  "dependencies": {
    "atom-select-list": "^0.8.1",
    "cson": "^7.20.0",
    "fuzzaldrin": "^2.1.0",
    "fuzzaldrin-plus": "^0.6.0",
    "glob": "^7.2.0"
  },
  "activationCommands": {
    "atom-workspace": [
      "project-files:prj-toggle",
      "project-files:prj-edit",
      "project-files:pth-toggle",
      "project-files:pth-recache"
    ],
    "atom-text-editor": [
      "project-files:open-TeX-PDF-internally",
      "project-files:open-TeX-PDF-externally"
    ]
  },
  "consumedServices": {
    "file-icons.element-icons": {
      "versions": {
        "1.0.0": "consumeElementIcons"
      }
    }
  },
  "license": "GNU GENERAL PUBLIC LICENSE Version 3, 29 June 2007"
}
